{
    "name": "konovaltsev/rv-base",
    "description": "My basic extension for Zend Framework 2",
    "keywords": [
        "zf2",
        "module",
        "extension"
    ],
    "authors": [
        {
            "name": "Roman V. Konovaltsev",
            "homepage": "http://konovaltsev.ru/"
        }
    ],
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "emuravjev/mdash",
                "version": "master",
                "source": {
                    "url": "http://github.com/emuravjev/mdash",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"pqr/superlib",
                "version":"1.2.3",
                "source":{
                    "type":"git",
                    "url":"http://github.com/pqr/superlib",
                    "reference":"master"
                },
                "autoload":{
                    "classmap":["timer.php"],
                    "files":["lib_functions.php"]
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "autoload": {
        "psr-0": {
            "RvBase\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "zendframework/zend-db":     "~2.3",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-navigation": "~2.3",
        "zendframework/zend-permissions-acl": "~2.3",
        "zendframework/zend-stdlib": "~2.3",
        "emuravjev/mdash": "master",
        "pqr/superlib":"1.2.3",
        "php":                       ">=5.4.0"
    },
    "require-dev": {
        "zendframework/zendframework": "~2.3",
        "pqr/superlib":"1.2.3",
        "emuravjev/mdash": "master"
    }
}
