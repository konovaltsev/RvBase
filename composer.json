{
    "name": "konovaltsev/rv-base",
    "description": "My basic extension for Zend Framework 2",
    "keywords": [
        "zf2",
        "module",
        "extension"
    ],
    "authors": [
        {
            "name": "Roman V. Konovaltsev",
            "homepage": "http://konovaltsev.ru/"
        }
    ],
    "repositories": [
        {
            "type":"git",
            "url":"http://github.com/konovaltsev/mdash"
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "autoload": {
        "psr-0": {
            "RvBase\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "konovaltsev/mdash": "dev-master",
        "zendframework/zend-db":     "~2.3",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-navigation": "~2.3",
        "zendframework/zend-permissions-acl": "~2.3",
        "zendframework/zend-stdlib": "~2.3",
        "php":                       ">=5.4.0"
    },
    "require-dev": {
        "konovaltsev/mdash": "dev-master",
        "zendframework/zendframework": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
